<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="spring.mapper.activityManagementMapper.ActivityManagementPeopleMiddleMapper">

    <insert id="addId">
        INSERT INTO retirement_activityManagementAndPeople (activity_id,people_id)
        VALUE
	    (#{activityId},#{items.peopleId})
    </insert>
    <delete id="deleteId" parameterType="String">
        delete from retirement_activityManagementAndPeople where activity_id=#{activityId}
        and people_id in
        <foreach item="item" collection="array" open="(" separator="," close=")">
            #{item}
        </foreach>
    </delete>
</mapper>